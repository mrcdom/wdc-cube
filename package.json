{
  "name": "cube-workspaces",
  "private": true,
  "scripts": {
    "clean": "rimraf \"libs/**/lib\"  \"libs/**/*.tsbuildinfo\"",
    "compile": "tsc -b tsconfig.build.json",
    "prepare": "npm run compile",
    "install-libs": "node ./scripts/dependence-install.js libs",
    "install-apps": "node ./scripts/dependence-install.js apps"
  },
  "devDependencies": {
    "@types/node": "^14.17.4",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/typescript-estree": "^4.32.0",
    "rimraf": "3.0.2",
    "ts-node": "^10.2.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.2",
    "eslint": "^7.32.0",
    "ansi-regex": ">=5.0.1"
  },
  "workspaces": [
    "libs/*"
  ],
  "dependencies": {
    "cache-manager": "^3.4.3",
    "class-transformer": "^0.4.0",
    "class-validator": "^0.13.1"
  }
}
